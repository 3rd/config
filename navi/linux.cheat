% linux

# journal (all)
journalctl -e

# journal (current boot)
journalctl -b -e

# journal (kernel)
journalctl -k -e

# journal (..warn)
journalctl -e -p 4

# journal (system)
journalctl --system -e

# journal (user)
journalctl --user -e

# query mime handler
XDG_UTILS_DEBUG_LEVEL=3 xdg-mime query default <mime-type>
