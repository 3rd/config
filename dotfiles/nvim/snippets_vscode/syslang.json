{
  "dd": {
    "prefix": "dd",
    "description": "\"date\"",
    "body": ["$CURRENT_YEAR.$CURRENT_MONTH.$CURRENT_DATE"]
  },
  "ddd": {
    "prefix": "ddd",
    "description": "\"date (pretty)\"",
    "body": ["$CURRENT_YEAR $CURRENT_MONTH_NAME_SHORT $CURRENT_DATE"]
  },
  "dt": {
    "prefix": "dt",
    "description": "\"datetime\"",
    "body": ["$CURRENT_YEAR.$CURRENT_MONTH.$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE"]
  },
  "tt": {
    "prefix": "tt",
    "description": "\"time\"",
    "body": ["$CURRENT_HOUR:$CURRENT_MINUTE"]
  },
  "hr": {
    "prefix": "hr",
    "description": "\"horizontal line\"",
    "body": ["---------------------------------------------------------------------", ""]
  },
  "@code": {
    "prefix": "@code",
    "description": "\"code block\"",
    "body": ["@code $1", "$0", "@end", ""]
  },
  "@project": {
    "prefix": "@project",
    "description": "project document",
    "body": ["@meta", "  title: $1", "  type: project", "@end", "$0"]
  },
  "@consume": {
    "prefix": "@consume",
    "description": "consume document",
    "body": ["@meta", "  title: $1", "  type: consume", "@end", "$0"]
  },
  "@person": {
    "prefix": "@person",
    "description": "person document",
    "body": ["@meta", "  title: $1", "  type: person", "@end", "$0"]
  }
}
