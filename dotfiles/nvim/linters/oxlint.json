{
  "env": {
    "browser": true
  },
  "settings": {},
  "plugins": ["jest", "vitest", "nextjs", "promise", "node", "security", "react-perf"],
  "extends": ["oxlint:all"],
  "rules": {
    "import/default": "error",
    "import/named": "error",
    "import/namespace": "error",
    "promise/no-new-statics": "error",
    "promise/valid-params": "error",
    "vitest/no-conditional-tests": "error",
    "eslint/no-await-in-loop": "error",
    "oxc/no-accumulating-spread": "error",
    "react_perf/jsx-no-jsx-as-prop": "off",
    "react_perf/jsx-no-new-array-as-prop": "off",
    "react_perf/jsx-no-new-function-as-prop": "off",
    "react_perf/jsx-no-new-object-as-prop": "off",
    "eslint/default-case": "error",
    "eslint/no-bitwise": "error",
    "eslint/no-div-regex": "error",
    "eslint/no-eq-null": "error",
    "eslint/no-eval": "error",
    "eslint/no-iterator": "error",
    "eslint/no-proto": "error",
    "eslint/no-regex-spaces": "error",
    "eslint/no-unsafe-optional-chaining": "error",
    "eslint/no-var": "error",
    "eslint/no-void": "error",
    "import/no-cycle": [
      "error",
      {
        "ignoreTypes": true
      }
    ],
    "oxc/bad-bitwise-operator": "error",
    "oxc/no-const-enum": "error",
    "promise/catch-or-return": "error",
    "promise/spec-only": "error",
    "react/button-has-type": "error",
    "react/no-unknown-property": "error",
    "typescript/no-dynamic-delete": "error",
    "typescript/no-import-type-side-effects": "error",
    "typescript/no-namespace": "error",
    "typescript/no-non-null-asserted-nullish-coalescing": "error",
    "typescript/prefer-literal-enum-member": "error",
    "unicorn/no-anonymous-default-export": "error",
    "unicorn/no-array-for-each": "error",
    "unicorn/no-length-as-slice-end": "error",
    "unicorn/no-magic-array-flat-depth": "error",
    "unicorn/no-nested-ternary": "error",
    "unicorn/prefer-modern-math-apis": "error",
    "unicorn/prefer-node-protocol": "error",
    "unicorn/prefer-number-properties": "error",
    "eslint/no-extend-native": "error",
    "eslint/no-new": "error",
    "eslint/no-unexpected-multiline": "error",
    "eslint/no-useless-concat": "error",
    "eslint/no-useless-constructor": "error",
    "import/no-duplicates": "error",
    "import/no-named-as-default": "error",
    "import/no-named-as-default-member": "error",
    "import/no-self-import": "error",
    "oxc/approx-constant": "error",
    "oxc/misrefactored-assign-op": "error",
    "oxc/no-async-endpoint-handlers": "error",
    "typescript/no-confusing-non-null-assertion": "error",
    "typescript/no-extraneous-class": "error",
    "typescript/no-unnecessary-type-constraint": "error",
    "unicorn/consistent-function-scoping": "error",
    "unicorn/prefer-add-event-listener": "error",
    "eslint/array-callback-return": "error",
    "eslint/eqeqeq": "error",
    "eslint/no-array-constructor": "error",
    "eslint/no-case-declarations": "error",
    "eslint/no-constructor-return": "error",
    "eslint/no-fallthrough": "error",
    "eslint/no-inner-declarations": "error",
    "eslint/no-new-wrappers": "error",
    "eslint/no-prototype-builtins": "error",
    "eslint/no-self-compare": "error",
    "eslint/radix": "error",
    "eslint/require-await": "error",
    "eslint/symbol-description": "error",
    "react/checked-requires-onchange-or-readonly": "error",
    "react/no-unescaped-entities": "error",
    "typescript/prefer-enum-initializers": "error",
    "unicorn/escape-case": "error",
    "unicorn/explicit-length-check": "error",
    "unicorn/new-for-builtins": "error",
    "unicorn/no-hex-escape": "error",
    "unicorn/no-instanceof-array": "error",
    "unicorn/no-lonely-if": "error",
    "unicorn/no-negated-condition": "error",
    "unicorn/no-negation-in-equality-check": "error",
    "unicorn/no-new-buffer": "error",
    "unicorn/no-object-as-default-parameter": "error",
    "unicorn/no-static-only-class": "error",
    "unicorn/no-this-assignment": "error",
    "unicorn/no-typeof-undefined": "error",
    "unicorn/no-unreadable-iife": "error",
    "unicorn/no-useless-promise-resolve-reject": "error",
    "unicorn/no-useless-switch-case": "error",
    "unicorn/no-useless-undefined": "error",
    "unicorn/prefer-array-flat": "error",
    "unicorn/prefer-array-some": "error",
    "unicorn/prefer-blob-reading-methods": "error",
    "unicorn/prefer-code-point": "error",
    "unicorn/prefer-date-now": "error",
    "unicorn/prefer-dom-node-append": "error",
    "unicorn/prefer-dom-node-dataset": "error",
    "unicorn/prefer-dom-node-remove": "error",
    "unicorn/prefer-event-target": "error",
    "unicorn/prefer-math-trunc": "error",
    "unicorn/prefer-native-coercion-functions": "error",
    "unicorn/prefer-prototype-methods": "error",
    "unicorn/prefer-query-selector": "error",
    "unicorn/prefer-regexp-test": "error",
    "unicorn/prefer-string-replace-all": "error",
    "unicorn/prefer-string-slice": "error",
    "unicorn/prefer-type-error": "error",
    "unicorn/require-number-to-fixed-digits-argument": "error",
    "eslint/default-case-last": "error",
    "eslint/guard-for-in": "error",
    "eslint/no-label-var": "error",
    "eslint/no-multi-str": "error",
    "eslint/no-new-func": "error",
    "eslint/prefer-exponentiation-operator": "error",
    "eslint/prefer-numeric-literals": "error",
    "eslint/sort-imports": [
      "error",
      {
        "ignoreCase": false,
        "ignoreDeclarationSort": true
      }
    ],
    "promise/param-names": "error",
    "promise/prefer-await-to-then": "off",
    "react/self-closing-comp": "error",
    "typescript/adjacent-overload-signatures": "error",
    "typescript/array-type": "error",
    "typescript/consistent-indexed-object-style": "error",
    "typescript/no-empty-interface": "error",
    "typescript/prefer-for-of": "error",
    "typescript/prefer-function-type": "error",
    "typescript/prefer-namespace-keyword": "error",
    "unicorn/catch-error-name": "error",
    "unicorn/error-message": "error",
    "unicorn/no-await-expression-member": "error",
    "unicorn/numeric-separators-style": "error",
    "unicorn/prefer-array-flat-map": "error",
    "unicorn/prefer-dom-node-text-content": "error",
    "unicorn/prefer-includes": "error",
    "unicorn/prefer-logical-operator-over-ternary": "error",
    "unicorn/prefer-modern-dom-apis": "error",
    "unicorn/prefer-optional-catch-binding": "error",
    "unicorn/prefer-reflect-apply": "error",
    "unicorn/prefer-spread": "error",
    "unicorn/prefer-string-trim-start-end": "error",
    "unicorn/prefer-structured-clone": "error",
    "unicorn/require-array-join-separator": "error",
    "unicorn/switch-case-braces": "error",
    "unicorn/text-encoding-identifier-case": "error",
    "unicorn/throw-new-error": "error",
    "vitest/no-import-node-test": "error",
    "vitest/prefer-each": "error",
    "vitest/prefer-to-be-falsy": "error",
    "vitest/prefer-to-be-object": "error",
    "vitest/prefer-to-be-truthy": "error",
    "eslint/constructor-super": "error",
    "eslint/getter-return": "error",
    "eslint/no-unreachable": "error",
    "import/export": "error",
    "promise/no-return-in-finally": "error",
    "react/rules-of-hooks": "error",
    "typescript/consistent-type-imports": "error"
  }
}
